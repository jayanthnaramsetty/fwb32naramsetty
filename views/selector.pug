extends layout

block content
  h1= title
  - var image_names= ["bluestar.jpg","bluestar1.jpg","orangestar.jpg","redstar.jpg","redstar1.jpg"]
  - for(let i=0;i<3;i++){
     - let randValue= Math.floor(Math.random()*5)
     img(src=`/images/${image_names[randValue]}` alt=`Image No ${randValue}`)

  - }